<?php

namespace App;

use Illuminate\Database\Eloquent\Model;

class <%= resource.capitalize %> extends Model
{
    public $rules = [
        <% attributes.each do |a| %>'<%= a[:name] %>' => '<%= a[:rules] %>',
        <% end %>
    ];

    protected $fillable = ['<%= attributes.map{|a| a[:name]}.join("', '") %>'];
}
